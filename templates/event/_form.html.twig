{{ form_start(form) }}
{% if form_errors(form) %}
    <div class="alert alert-danger" role="alert">
        {{ form_errors(form) }}
    </div>
{% endif %}

<div class="mb-3">

    {{ form_row(form.title) }}
</div>
<div class="mb-3">
    {{ form_row(form.description) }}
</div>
<div class="mb-3">
    {{ form_row(form.sdate, {
            label: 'Start Date'
        }) }}
</div>
<div class="mb-3">
    {{ form_row(form.edate, {
            label: 'End Date'
        }) }}

</div>

<div class="col-12">
    <button class="btn  btn-primary">
        {{ button_label|default('Submit') }}
    </button>

</div>

{{ form_end(form) }}
